class Sample {
    public static void main(String[] args) {
        int a=7,b=6;
        System.out.println((a>b) && (a!=b));
        System.out.println((a<b) ||(a==b));
        System.out.println(!((a>b) || (a==b)));
        System.out.println(11&3) ;
        System.out.println(11|3);
        System.out.println(11^3);
        System.out.println(11>>3);
        System.out.println(11<<3);
    }
}
